{
    "features": [
        {
            "name": "dbs",
            "templateType": "HBS",
            "entityPageSet": {
                "htmlTemplate": "database",
                "urlTemplate": "database_url"
            },
            "streamId": "databases"
        },
        {
            "name": "tables",
            "templateType": "HBS",
            "entityPageSet": {
                "htmlTemplate": "table",
                "urlTemplate": "table_url"
            },
            "streamId": "tables"
        }
    ],
    "streams": [
        {
            "$id": "databases",
            "source": "knowledgeGraph",
            "destination": "pages",
            "fields": [
                "name",
                "id",
                "c_tables",
                "c_owningTeam",
                "c_owningService"
            ],
            "filter": {
                "entityTypes": [
                    "ce_database"
                ]
            },
            "localization": {
                "locales": ["en"]
            }
        },
        {
            "$id": "tables",
            "source": "knowledgeGraph",
            "destination": "pages",
            "fields": [
                "name",
                "id",
                "c_columns"
            ],
            "filter": {
                "entityTypes": [
                    "ce_table"
                ]
            },
            "localization": {
                "locales": ["en"]
            }
        }
    ]
}